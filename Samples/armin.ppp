
procedure chaap(a : integer; b : string)
begin
    write(b);
    write("\n");
    b := "shir";
    write(b);
    write("\n");
    write(a + 'a' = 2 * a);
end

function chaapgar(): string
begin
    return "to az tolooye sobhi too shere eltemasam.";
end


function main() : integer
begin
    chaap('a', "salam");
    chaapgar();
    return 0 ;
end
